
.nav-left>view,
.nav-right>view {
	font-size: 40rpx;
}
.nav-left>view {
	color: #FEAD00;
}
.nav-right>view {
	color: #333333;
}
.nav-left {
	margin-left: 16rpx;
}
.nav-right {
	/* margin-right: -50upx; */
}
.nav-tab-bar {
	width: 100%;
	margin-left: -20rpx;
	position: relative;
}
.nav-tab-bar>view {
	font-size: 32rpx;
	padding: 0 15rpx;
	font-weight: bold;
	color: #969696;
}
.active {
	color: #333333 !important;
}
.nav-tab-bar-line {
	position: absolute;
	border-bottom: 5rpx solid #FEDE33;
	border-top: 5rpx solid #FEDE33;
	width: 70rpx;
	border-radius: 20rpx;
	bottom: 8rpx;
}



